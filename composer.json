{
    "name": "Vasatiani/laravel-athena",
    "description": "Laravel Athena query builder integration with Eloquent support and Redis-safe concurrency.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vakho Asatiani",
            "email": "vasatiani@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/redis": "^10.0|^11.0|^12.0",
        "aws/aws-sdk-php": "^3.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "goodby/csv": "^1.3",
        "predis/predis": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Vasatiani\\Athena\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vasatiani\\Athena\\AthenaServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
